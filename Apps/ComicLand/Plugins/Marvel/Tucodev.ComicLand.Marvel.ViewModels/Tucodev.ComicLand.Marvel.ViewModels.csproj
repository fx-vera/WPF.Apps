<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
    <ImplicitUsings>enable</ImplicitUsings>
    <SignAssembly>True</SignAssembly>
    <AssemblyOriginatorKeyFile>..\..\..\Tucodev.Apps.snk</AssemblyOriginatorKeyFile>
    <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
    <RepositoryUrl>https://github.com/fx-vera/tucodev-WPF/tree/master/Apps/ComicLand</RepositoryUrl>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Copyright>2024 fxvera (tucodev)</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\..\..\LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="PropertyChanged.Fody" Version="4.1.0" />
    <PackageReference Include="System.ComponentModel.Composition" Version="8.0.0" />
    <PackageReference Include="tucodev.Core" Version="1.0.1" />
    <PackageReference Include="tucodev.WPF.Core" Version="1.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Tucodev.ComicLand.Marvel.Logic\Tucodev.ComicLand.Marvel.Logic.csproj" />
    <ProjectReference Include="..\Tucodev.ComicLand.Marvel.Models\Tucodev.ComicLand.Marvel.Models.csproj" />
    <ProjectReference Include="..\Tucodev.ComicLand.Marvel.Views\Tucodev.ComicLand.Marvel.Views.csproj" />
  </ItemGroup>

</Project>
